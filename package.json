{
  "name": "ethjs-ens",
  "version": "2.0.1",
  "description": "An ENS interface module built on EthJS.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/index.js",
    "test": "npm run lint && istanbul cover tape test/*.js && npm run coveralls",
    "build": "browserify example/index.js -o example/bundle.js",
    "prebuild": "npm run build:babel",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "build:babel": "babel src --out-dir dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ethjs/ethjs-ens.git"
  },
  "keywords": [
    "Ethereum",
    "DNS",
    "ENS",
    "EthJS"
  ],
  "author": "Dan Finlay",
  "contributors": [
    "jchimien@gmail.com"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ethjs/ethjs-ens/issues"
  },
  "homepage": "https://github.com/ethjs/ethjs-ens#readme",
  "dependencies": {
    "eth-ens-namehash": "^1.0.2",
    "ethereum-ens-network-map": "^1.0.0",
    "ethjs-contract": "^0.1.7",
    "ethjs-query": "^0.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browserify": "^16.2.2",
    "coveralls": "^2.12.0",
    "eslint": "^4.7.1",
    "ethereumjs-testrpc": "^3.0.3",
    "ethjs": "^0.2.5",
    "ethjs-provider-http": "^0.1.6",
    "ethjs-query": "^0.2.4",
    "istanbul": "^0.4.5",
    "sinon": "^2.1.0",
    "solc": "^0.4.9",
    "tape": "^4.6.3",
    "web3": "^0.18.2"
  }
}
